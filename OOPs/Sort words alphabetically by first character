class Sorttxt:
    def _init_(self,txt):
            if isinstance(txt,Sorttxt):
                self.txt = txt.txt
                  
            else:
                self.txt = txt

    def __display(self):
            a = self.txt
            c = a.split(" ")
            b = sorted(c)
            return b
    def res(self):
            print(self.__display())
    
obj = Sorttxt("Red Green Black White Pink")
 obj.display()
obj.res()
obj2 = Sorttxt(obj)
