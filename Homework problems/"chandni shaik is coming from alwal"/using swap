words = "chandni shaik is coming from alwal"
words_split = words.split()
result = []
for word in words_split:
    if len(word) > 3:
        c = list(word)
        swap=[(0,2),(1,3),(4,6)]
        for i,j in swap:
            if j < len(c):
                c[i], c[j] = c[j], c[i]
        result.append(''.join(c))
    else:
        result.append(word)
replaced=' '.join(result)
print(replaced)
